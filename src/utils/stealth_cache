# src/utils/stealth_cache.py (NEW FILE)
"""Caching layer for stealth operations"""

class StealthCache:
    def __init__(self):
        self._profile_cache = {}
        self._fingerprint_cache = {}
        self._script_cache = {}
    
    async def get_or_generate_profile(self, key: str, generator_func):
        if key not in self._profile_cache:
            self._profile_cache[key] = await generator_func()
        return self._profile_cache[key]